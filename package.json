{
  "name": "bridgedb",
  "version": "6.0.0-14",
  "description": "JS client for BridgeDb.org ID mapping framework webservices",
  "main": "lib/index.js",
  "main:esnext": "esnext/index.js",
  "typings": "lib/index.d.ts",
  "bin": "./bin/bridgedb",
  "files": [
    "lib/",
    "src/",
    "tsconfig.json",
    "tsconfig.base.json",
    "tsconfig.demo.json",
    "tsconfig.esnext.json",
    "typings/index.d.ts",
    "typings/organism.d.ts"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run compile && mkdirp dist && rm -rf dist/* && browserify ./lib/index.js -o ./dist/index.js -d -t cssify -t deglobalify",
    "build-demo": "npm run compile:es5:ui && npm run compile:es5:ui:test && browserify ./test/e2e/ui-components-local.test.js -o ./demo/ui-combo.bundle.js -d -t deglobalify",
    "compile": "npm-run-all -p compile:*",
    "compile:es5": "mkdirp lib && rm -rf lib/* && tsc --project tsconfig.json && cp ./node_modules/react-selectize/themes/default.css ./lib/components/react-selectize-theme-default.css && echo ok || echo not ok",
    "compile:esnext": "mkdirp esnext && rm -rf esnext/* && tsc --project tsconfig.esnext.json && cp ./node_modules/react-selectize/themes/default.css ./esnext/components/react-selectize-theme-default.css && echo ok || echo not ok",
    "compile-demo:es5:ui": "tsc src/components/react-demo.tsx --jsx react --sourceMap --lib dom,es5,scripthost --module commonjs --outDir components/ && echo ok || echo not ok",
    "compile-demo:es5:ui:test": "tsc --project tsconfig.demo.json && echo ok || echo not ok",
    "dev": "npm run build:es5",
    "start": "npm-run-all -p start:mockserver start:ui",
    "start:mockserver": "./node_modules/mockserver/bin/mockserver.js -p 4522 -m './test/input-data'",
    "start:ui": "webpack-dev-server --config webpack.config.js --progress --colors --watch",
    "test": "echo 'add working tests'",
    "watch": "npm-run-all -p start:mockserver watch:*",
    "watch:client": "npm-watch",
    "watch:ui": "npm run start:ui"
  },
  "watch": {
    "dev": [
      "index.d.ts",
      "src/*.{ts,tsx}",
      "test/*.{ts,tsx,js,jsx}"
    ],
    "test": [
      "index.js",
      "ui/*.{js,jsx}",
      "test/*.{js,jsx}"
    ]
  },
  "browser": {
    "./lib/placeholder": "./lib/components/react-selectize-theme-default.css"
  },
  "browserify": {
    "transform": [
      "deglobalify",
      "cssify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bridgedb/bridgedbjs.git"
  },
  "keywords": [
    "bridgedb",
    "biology"
  ],
  "author": "BridgeDb team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bridgedb/bridgedbjs/issues"
  },
  "homepage": "https://bridgedb.github.io/bridgedbjs/docs/",
  "dependencies": {
    "JSONStream": "^1.0.3",
    "browserfs": "^1.1.0",
    "commander": "^2.9.0",
    "csstips": "^0.2.0",
    "csv-streamify": "^1.0.0",
    "csx": "^8.2.1",
    "deglobalify": "^0.2.0",
    "highland": "~2.5.1",
    "insert-css": "^0.2.0",
    "json-loader": "^0.5.4",
    "levenshtein": "^1.0.5",
    "lodash": "^4.11.1",
    "ndjson": "^1.5.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dom": "^15.3.2",
    "react-icons-kit": "^1.0.4",
    "react-redux": "^4.4.5",
    "react-selectize": "^2.1.0",
    "redux": "^3.6.0",
    "redux-form": "^6.1.1",
    "redux-localstorage": "^0.4.1",
    "rx-extra": "^2.0.4",
    "rxjs": "^5.0.0-rc.2",
    "typestyle": "^0.22.3",
    "urijs": "^1.18.1",
    "verror": "^1.9.0",
    "xhr2": "^0.1.3"
  },
  "devDependencies": {
    "@types/assert": "0.0.28",
    "@types/jsonstream": "^0.8.27",
    "@types/lodash": "^4.14.36",
    "@types/node": "^6.0.58",
    "@types/react": "^15.0.14",
    "@types/react-dom": "^0.14.23",
    "@types/rx": "^4.1.1",
    "@types/urijs": "^1.15.27",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "bdd-with-opts": "~1.0.0",
    "blessed": "^0.1.81",
    "browser-sync": "^2.13.0",
    "browserify": "^14.1.0",
    "budo": "^8.2.2",
    "chai": "~1.10.0",
    "chai-as-promised": "~4.1.1",
    "colors": "~1.0.3",
    "css-loader": "^0.26.1",
    "cssify": "^1.0.3",
    "deep-diff": "^0.3.3",
    "deglobalify": "^0.2.0",
    "freeport": "~1.0.4",
    "global": "^4.3.0",
    "graceful-fs": "^4.1.4",
    "html-webpack-plugin": "^2.24.0",
    "inquirer": "~0.8.0",
    "jaguarjs-jsdoc": "0.0.1",
    "jquery": "^2.2.3",
    "jsdoc": "^3.4.0",
    "jsdom": "^8.4.0",
    "jsonld-markup": "^0.1.1",
    "jsonld-vis": "^0.2.0",
    "karma": "0.13.22",
    "karma-browserify": "^5.1.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-firefox-launcher": "0.1.7",
    "karma-growl-reporter": "0.1.1",
    "karma-mocha": "0.2.2",
    "karma-mocha-reporter": "2.0.0",
    "mkdirp": "~0.5.1",
    "mocha": "^2.5.3",
    "mockserver": "^1.5.1",
    "npm-run-all": "^2.3.0",
    "npm-watch": "^0.1.6",
    "pretty-data": "~0.40.0",
    "pretty-hrtime": "~0.2.2",
    "react-addons-test-utils": "^15.3.2",
    "rsyncwrapper": "~0.4.1",
    "rx-fs": "ariutta/rx-fs#03425b37820b71ab4e8947a6e14fa5ef8135e990",
    "sax": "~0.6.1",
    "selenium-launcher": "~1.1.10",
    "sinon": "~1.12.1",
    "spawn-mocha-parallel": "~1.0.0",
    "style-loader": "^0.13.1",
    "through": "~2.3.6",
    "tower-strcase": "~0.5.0",
    "ts-loader": "^1.3.3",
    "tsify": "^3.0.1",
    "typescript": "~2.1.5",
    "vinyl": "~0.4.5",
    "vinyl-buffer": "~1.0.0",
    "vinyl-fs": "^2.4.3",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "^3.7.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.0",
    "yargs": "~1.3.3"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./gulp",
        "./dist"
      ]
    }
  }
}
