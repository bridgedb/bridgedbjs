{
  "name": "bridgedb",
  "version": "5.0.13",
  "description": "JS client for BridgeDb.org ID mapping framework webservices",
  "main": "index.js",
  "scripts": {
    "build": "browserify ./src/main.dummy.ts -o ./dist/bridgedb.js -d -p [ tsify --target 'es6' ] -t [ babelify --extensions '.ts' --extensions '.tsx' ] -t brfs -t deglobalify",
    "build:demo": "browserify ./test/e2e/ui-components.test.js -o ./demo/ui-combo.bundle.js -d -t brfs -t deglobalify",
    "start": "npm-run-all -p start:mockserver start:ui-combo-local",
    "start:mockserver": "./node_modules/mockserver/bin/mockserver.js -p 4522 -m './test/input-data'",
    "start:ui-combo-local": "budo test/e2e/ui-components-local.test.js --live --open -- -t brfs -t deglobalify",
    "start:ui-combo-remote": "budo test/e2e/ui-components-remote.test.js --live --open -- -t brfs -t deglobalify",
    "start:ui-datasource": "budo test/e2e/datasource-control.test.js --live --open -- -t brfs",
    "start:ui-identifier": "budo test/e2e/identifier-control.test.js --live --open -- -t brfs",
    "test.broken": "npm-run-all test:*",
    "test1": "mocha --harmony --full-trace --check-leaks --timeout=6000",
    "test": "mocha --harmony --full-trace --check-leaks --timeout=6000 ./test/unit/dataset/get.js",
    "test:Dataset": "gulp testDataset",
    "test:EntityReference": "gulp testEntityReference",
    "test:Organism": "gulp testOrganism",
    "test:Xref": "gulp testXref",
    "test-broken:ui": "NODE_ENV=test karma start:mockserver",
    "test-broken:ui-combo": "NODE_ENV=test karma start:mockserver karma-ui-combo.conf.js"
  },
  "browserify": {
    "transform": [
      "brfs",
      "deglobalify"
    ]
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bridgedb/bridgedbjs.git"
  },
  "keywords": [
    "bridgedb",
    "biology"
  ],
  "author": "BridgeDb team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bridgedb/bridgedbjs/issues"
  },
  "homepage": "https://bridgedb.github.io/bridgedbjs/docs/",
  "dependencies": {
    "@types/assert": "0.0.28",
    "@types/jsonstream": "^0.8.27",
    "@types/lodash": "^4.14.31",
    "@types/rx": "^2.5.29",
    "@types/rx-node": "0.0.1",
    "@types/urijs": "^1.15.27",
    "JSONStream": "^1.0.3",
    "csv-streamify": "~1.0.0",
    "hyperquest": "^1.2.x",
    "insert-css": "^0.2.0",
    "jsonld-rx-extra": "git://github.com/ariutta/jsonld-rx-extra#961f6543aa160e0595b6e91d6261a90b27dfa67b",
    "levenshtein": "^1.0.5",
    "lodash": "^4.11.1",
    "rx-extra": "git://github.com/ariutta/rx-extra.git#117359e7101c328187c648637c38c69aa60751d2",
    "urijs": "^1.18.1",
    "yolk": "0.10.1",
    "yolk-simple-modal": "~0.0.4"
  },
  "devDependencies": {
    "RxNodewd": "~0.3.11",
    "babelify": "^7.3.0",
    "bdd-with-opts": "~1.0.0",
    "blessed": "^0.1.81",
    "brfs": "^1.4.3",
    "browser-sync": "^2.13.0",
    "browserify": "^12.0.2",
    "budo": "^8.2.2",
    "chai": "~1.10.0",
    "chai-as-promised": "~4.1.1",
    "colors": "~1.0.3",
    "deep-diff": "^0.3.3",
    "deglobalify": "^0.2.0",
    "freeport": "~1.0.4",
    "global": "^4.3.0",
    "graceful-fs": "^4.1.4",
    "highland": "~2.5.1",
    "inquirer": "~0.8.0",
    "jaguarjs-jsdoc": "0.0.1",
    "jquery": "^2.2.3",
    "jsdoc": "^3.4.0",
    "jsdom": "^8.4.0",
    "jsonld-markup": "^0.1.1",
    "jsonld-vis": "^0.2.0",
    "karma": "0.13.22",
    "karma-browserify": "^5.1.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-firefox-launcher": "0.1.7",
    "karma-growl-reporter": "0.1.1",
    "karma-mocha": "0.2.2",
    "karma-mocha-reporter": "2.0.0",
    "mkdirp": "~0.5.1",
    "mocha": "^2.5.3",
    "mockserver": "^1.5.1",
    "npm-run-all": "^2.1.1",
    "pretty-data": "~0.40.0",
    "pretty-hrtime": "~0.2.2",
    "rsyncwrapper": "~0.4.1",
    "rx-fs": "ariutta/rx-fs#03425b37820b71ab4e8947a6e14fa5ef8135e990",
    "sax": "~0.6.1",
    "selenium-launcher": "~1.1.10",
    "sinon": "~1.12.1",
    "spawn-mocha-parallel": "~1.0.0",
    "through": "~2.3.6",
    "tower-strcase": "~0.5.0",
    "tsify": "^1.0.3",
    "typescript": "^2.1.0-dev.20160816",
    "vinyl": "~0.4.5",
    "vinyl-buffer": "~1.0.0",
    "vinyl-fs": "^2.4.3",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "^3.7.0",
    "yargs": "~1.3.3"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./gulp",
        "./dist"
      ]
    }
  }
}
