{
  "name": "bridgedb",
  "version": "5.0.13",
  "description": "JS client for BridgeDb.org ID mapping framework webservices",
  "main": "index.js",
  "scripts": {
    "build:demo": "browserify ./test/e2e/ui-components.test.js -o ./demo/ui-combo.bundle.js -d -t brfs -t deglobalify",
    "manual": "budo test/e2e/ui-components.test.js --live --open -- -t brfs -t deglobalify",
    "manual:datasource": "budo test/e2e/datasource-control.test.js --live --open -- -t brfs",
    "manual:identifier": "budo test/e2e/identifier-control.test.js --live --open -- -t brfs",
    "start": "./node_modules/mockserver/bin/mockserver.js -p 4522 -m './test/input-data'",
    "test": "npm-run-all test:*",
    "test:Dataset": "gulp testDataset",
    "test:EntityReference": "gulp testEntityReference",
    "test:Organism": "gulp testOrganism",
    "test:Xref": "gulp testXref",
    "test-broken:ui": "NODE_ENV=test karma start",
    "test-broken:ui-combo": "NODE_ENV=test karma start karma-ui-combo.conf.js"
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bridgedb/bridgedbjs.git"
  },
  "keywords": [
    "bridgedb",
    "biology"
  ],
  "author": "BridgeDb team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bridgedb/bridgedbjs/issues"
  },
  "homepage": "https://bridgedb.github.io/bridgedbjs/docs/",
  "dependencies": {
    "brfs": "^1.4.3",
    "csv-streamify": "~1.0.0",
    "gulp": "^3.8.11",
    "hyperquest": "^1.2.x",
    "insert-css": "^0.2.0",
    "jsonld-rx-extra": "git://github.com/ariutta/jsonld-rx-extra#eff25cedd4c007fb3f59e2d2d1ff07c53a3bd0ec",
    "jsonstream": "^1.0.3",
    "levenshtein": "^1.0.5",
    "lodash": "^4.11.1",
    "rx-extra": "git://github.com/ariutta/rx-extra.git#d8ef18b1af76371f49cf1334cdf24e39bed1dd3d",
    "urijs": "^1.15.1",
    "yolk": "0.10.1",
    "yolk-simple-modal": "~0.0.3"
  },
  "devDependencies": {
    "bdd-with-opts": "~1.0.0",
    "blessed": "^0.1.81",
    "browser-sync": "~2.5.3",
    "browserify": "^12.0.2",
    "budo": "^8.2.2",
    "chai": "~1.10.0",
    "chai-as-promised": "~4.1.1",
    "colors": "~1.0.3",
    "deep-diff": "^0.3.3",
    "deglobalify": "^0.2.0",
    "freeport": "~1.0.4",
    "global": "^4.3.0",
    "gulp": "~3.8.10",
    "gulp-brfs": "~0.1.0",
    "gulp-bump": "~0.2.2",
    "gulp-git": "~1.1.0",
    "gulp-jsdoc": "~0.1.4",
    "gulp-jshint": "~1.11.0",
    "gulp-mocha": "~2.0.0",
    "gulp-modernizr": "~1.0.0-alpha",
    "gulp-notify": "~2.1.0",
    "gulp-prompt": "~0.1.1",
    "gulp-regex-replace": "~0.2.3",
    "gulp-rename": "~1.2.2",
    "gulp-run": "~1.6.4",
    "gulp-sequence": "~0.3.1",
    "gulp-sourcemaps": "~1.5.2",
    "gulp-uglify": "~1.2.0",
    "gulp-util": "~3.0.1",
    "highland": "~2.5.1",
    "inquirer": "~0.8.0",
    "jaguarjs-jsdoc": "0.0.1",
    "jquery": "^2.2.3",
    "jscs": "~1.7.3",
    "jsdom": "^8.4.0",
    "jsonld-markup": "^0.1.1",
    "jsonld-vis": "^0.2.0",
    "karma": "0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-coverage": "0.5.5",
    "karma-firefox-launcher": "0.1.7",
    "karma-growl-reporter": "0.1.1",
    "karma-mocha": "0.2.2",
    "karma-mocha-reporter": "2.0.0",
    "mkdirp": "~0.5.1",
    "mocha": "~2.0.1",
    "mockserver": "^1.5.1",
    "npm-run-all": "^2.1.1",
    "pretty-data": "~0.40.0",
    "pretty-hrtime": "~0.2.2",
    "rsyncwrapper": "~0.4.1",
    "rx-fs": "ariutta/rx-fs#03425b37820b71ab4e8947a6e14fa5ef8135e990",
    "sax": "~0.6.1",
    "selenium-launcher": "~1.1.10",
    "sinon": "~1.12.1",
    "spawn-mocha-parallel": "~1.0.0",
    "through": "~2.3.6",
    "tower-strcase": "~0.5.0",
    "vinyl": "~0.4.5",
    "vinyl-buffer": "~1.0.0",
    "vinyl-fs": "~0.3.13",
    "vinyl-source-stream": "~1.0.0",
    "watchify": "^2.6.2",
    "wd": "~0.3.11",
    "yargs": "~1.3.3"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./gulp",
        "./dist"
      ]
    }
  }
}
